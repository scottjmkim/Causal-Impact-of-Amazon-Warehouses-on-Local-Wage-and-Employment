   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "master = pd.merge(df_total_employment, df_income,\n",
    "                 how = \"inner\",\n",
    "                 on = [\"GeoFips\", \"Year\", \"GeoName\"],\n",
    "                 indicator = True)"
